        "def cipher(str):\n",
        "  ans = \"\"\n",
        "  for s in str:\n",
        "    if s.islower():\n",
        "      ans += chr(219-ord(s))\n",
        "    else:\n",
        "      ans += s\n",
        "  return ans\n",
        "\n",
        "text = cipher(\"Use this function to cipher and decipher an English message.\")\n",
