        "import re\n",
        "s = \"Now I need a drink, alcoholic of course, after the heavy lectures involving quantum mechanics.\"\n",
        "s = re.sub('[,\\.]', '', s)\n",
        "ans = []\n",
        "words = s.split()\n",
        "for w in words:\n",
        "  ans.append(len(w))\n",
        "print(ans)"
